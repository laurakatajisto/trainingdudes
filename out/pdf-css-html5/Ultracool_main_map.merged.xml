            <featnum class="- topic/featnum ">VR-12321</featnum>
            <platform class="- topic/platform ">Windows</platform>
            <prognum class="- topic/prognum ">RT-5234</prognum>
        </prodinfo></metadata>
    </prolog>
    <taskbody class="- topic/body task/taskbody ">
        <context audience="expert" class="- topic/section task/context " id="unique_6_Connect_42_context_c1m_pmx_s4b" nd:nd-id="context_c1m_pmx_s4b">We added some context for experts.<note class="- topic/note " type="trouble">Do something.</note></context>
        <prereq audience="novice" class="- topic/section task/prereq " id="unique_6_Connect_42_prereq_a13_qmx_s4b" nd:nd-id="prereq_a13_qmx_s4b">And prerequisites for novices
            only.</prereq>
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Remove the filter from its packaging.</cmd>
                <info class="- topic/itemgroup task/info ">
                    <note class="- topic/note " type="important">Watch out for slippery roads.</note>
                </info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Lift the water reservoir cover.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Press the <uicontrol class="+ topic/ph ui-d/uicontrol ">Open</uicontrol> button.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Pull the filter compartment towards you.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Place the filter to the compartment carefully.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Push the filter compartment away from you.</cmd>
            </step>
        </steps>
    </taskbody>
