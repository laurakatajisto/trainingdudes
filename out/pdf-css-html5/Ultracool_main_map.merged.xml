<task class="- topic/topic task/task " id="unique_6" topicrefclass="- map/topicref " nd:nd-id="task_insert_water_filter" oid="task_insert_water_filter" specializations="@props/deliveryTarget">
    <opentopic-index:refID indexid="indexid1904344505" value="water filter:" class="- index/refid " id="d31e972"/><opentopic-index:refID indexid="indexid-555220317" value="insert the water filter:" class="- index/refid " id="d31e976"/><title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">Insert the water filter</ph></title>
    <shortdesc class="- topic/shortdesc ">The water filter purifies the tap water before brewing the coffee or making the
        tea.</shortdesc>
