    <taskbody class="- topic/body task/taskbody ">
        <context class="- topic/section task/context " id="unique_3_Connect_42_context_omy_4ws_t4b" nd:nd-id="context_omy_4ws_t4b">
            <p class="- topic/p ">Here is information about the task.</p>
            <p class="- topic/p ">This product can only be serviced by qualified
            professionals.</p>
            
        </context>
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Press <uicontrol class="+ topic/ph ui-d/uicontrol ">Shut down</uicontrol> to switch off the <ph class="- topic/ph " keyref="product-name">Ultracool 7000</ph> Coffee and Tea Machine.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Turn the grinder lock switch left until the <ph class="- topic/ph " keyref="product-name">Ultracool 7000</ph> display
                    shows <uicontrol class="+ topic/ph ui-d/uicontrol ">Open</uicontrol>.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Lift the grinder cover.</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">Pour in the coffee beans.</cmd>
                <info audience="CEO" class="- topic/itemgroup task/info ">When the grinder has enough beans, the display shows
                        <uicontrol class="+ topic/ph ui-d/uicontrol ">Full</uicontrol>.</info>
            </step>
        </steps>
    </taskbody>
</task></topic><concept xmlns:dita-ot="http://dita-ot.sourceforge.net/ns/201007/dita-ot" break-before="true" is-chapter="true" class="- topic/topic concept/concept " id="unique_5" topicrefclass="- map/topicref " nd:nd-id="concept_vvb_txy_t4b" oid="concept_vvb_txy_t4b" specializations="@props/deliveryTarget">
